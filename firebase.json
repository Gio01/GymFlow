{
  "hosting": {
    "public": "public",
    "rewrites":[{
        "source" : "/",
        "destination": "/index.ejs"
  },{
        "source" : "/fitness",
        "destination" : "/fitness.ejs"
  },{
       "source" : "/contact",
       "destination" : "/contact.ejs"
  },{
       "source" : "/login",
       "destination" : "/login.ejs"
  },{
       "source" : "/profile",
       "destination" : "/profile.ejs"
  },{
       "source" : "/register",
       "destination" : "/register.ejs"
  },{
      "source" : "/program",
      "destination" : "/program.ejs"
  }],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": {
    "predeploy": [],
    "source": "functions"
  },
  "emulators": {
    "functions": {
      "port": 5001
    },
    "firestore": {
      "port": 8080
    },
    "hosting": {
      "port": 5000
    },
    "ui": {
      "enabled": true
    }
  }
}
